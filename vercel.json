{
    "version": 2,
    "builds": [{ "src": "package.json", "use": "@now/next" }],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "api/$1",
        "methods": ["OPTIONS", "GET", "POST", "PUT", "DELETE"],
        "headers": {
          "Access-Control-Allow-Origin": "*",
          "Access-Control-Allow-Methods": "OPTIONS, GET, POST, PUT, DELETE",
          "Access-Control-Allow-Headers": "Content-Type"
        }
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }
  